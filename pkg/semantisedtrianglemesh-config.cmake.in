# Compute installation prefix relative to this file.
get_filename_component(_dir "${CMAKE_CURRENT_LIST_FILE}" PATH)
get_filename_component(_prefix "${_dir}/../.." ABSOLUTE)

# Import the targets.
include("${_prefix}/lib/TriangleMesh-@version@/TriangleMesh-targets.cmake")

# Report other information.
set(TriangleMesh_INCLUDE_DIRS "${_prefix}/include/TriangleMesh-@version@")

file(GLOB TriangleMesh_LIBRARIES "${_prefix}/lib/TriangleMesh-@version@/lib*")
